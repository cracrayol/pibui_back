{
  "name": "pibui_backend",
  "version": "1.0.0",
  "description": "Backend part of Pibui",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "tsc",
    "serve": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/index.js",
    "lint": "tslint --project ./tslint.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "connect-session-sequelize": "^5.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "mysql2": "^1.5.1",
    "sequelize": "^4.31.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.0",
    "@types/express-validator": "^3.0.0",
    "@types/express-session": "^1.15.6",
    "@types/express-jwt": "^0.0.38",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/node": "^9.3.0",
    "@types/sequelize": "^4.27.2",
    "concurrently": "^3.5.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
